{
  "name": "dojo-timer",
  "version": "1.0.0",
  "description": "dojo timer for the win",
  "main": "electron/electron.js",
  "license": "MIT",
  "extends": null,
  "homepage": "./",
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "electron/**/*",
      "dist/**/*"
    ],
    "directories": {
      "output": "electron-build"
    }
  },
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:8080 && electron .\"",
    "start": "webpack-dev-server --mode development --config config/webpack.dev.config.js --hot",
    "build": "webpack --mode production --config config/webpack.prod.config.js --env.NODE_ENV=production --progress",
    "electron-pack": "build -c.extraMetadata.main=electron/electron.js",
    "rebuild": "electron-rebuild",
    "preelectron-pack": "yarn rebuild && yarn build ",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint src/**/*.js",
    "lint:scss": "npx stylelint src/**/*.scss"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "electron": "^5.0.10",
    "electron-builder": "^20.44.4",
    "electron-rebuild": "^1.8.5",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "prettier": "^1.18.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^3.0.0",
    "stylelint-prettier": "^1.1.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "wait-on": "^3.1.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "electron-is-dev": "^1.0.1",
    "fs": "0.0.1-security",
    "moment": "^2.24.0",
    "net": "^1.0.2",
    "node-pty": "^0.8.1",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "webpack-node-externals": "^1.7.2",
    "xterm": "^3.14.5",
    "xterm-addon-fit": "^0.1.0"
  }
}
